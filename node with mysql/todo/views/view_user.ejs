<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>View All Users</title>
</head>
<body>
   <h1>View All Users</h1>
   <table border="2">
       <tr>
           <th>ID</th>
           <th>Name</th>
           <th>Password</th>
       </tr>
       <% result.forEach(function(data) { %>
           <tr>
               <td><%= data.id %></td>
               <td><%= data.username %></td>
               <td><%= data.password %></td>
               <a href=""></a>
           </tr>
       <% }); %>
   </table>
</body>
</html>
